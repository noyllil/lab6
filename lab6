#include <iostream>
#include <iomanip>
#include <windows.h>
#include "math.h"
#include <conio.h>
using namespace std;
int main()
{
	double x, y, h, a, b;
	cout << "Enter a:";
	cin >> a;
	cout << "Enter b:";
	cin >> b;
	cout << "Enter h:";
	cin >> h;
	x = a;
	do
	{
		y = sin(x), 2 + cos(x), 2;
		cout << setw(5) << x << '\t' << y << endl;
		x += h;
	}while (x <= b);
	return 0;
}
